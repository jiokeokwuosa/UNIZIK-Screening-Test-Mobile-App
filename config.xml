<?xml version='1.0' encoding='utf-8'?>
<widget id="org.Godslove.UnizikScreeningSci018" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UnizikScreeningSci018</name>
    <description>
        A Mobile Screening test App
    </description>
    <author email="okwuosachijioke@gmail.com" href="http://cordova.io">
        Okwuosa Chijioke
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-dialogs" spec="D:\mobile\cordova plugins\cordova-plugin-dialogs" />
    <plugin name="cordova-plugin-network-information" spec="D:\mobile\cordova plugins\cordova-plugin-network-information" />
    <engine name="android" spec="^7.0.0" />
</widget>
